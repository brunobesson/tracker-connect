{
  "name": "tracker-connect",
  "version": "1.0.0",
  "description": "Tiny server to handle connection with activity trackers such as Strava, Garmin or Suunto",
  "main": "dist/server.js",
  "scripts": {
    "start": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/server.ts | pino-pretty\"",
    "build": "tsc",
    "lint": "prettier --check \"**/*.{ts,js,json,css,scss,less,md,html}\" && eslint src/**",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunobesson/tracker-connect.git"
  },
  "author": "https://www.camptocamp.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brunobesson/tracker-connect/issues"
  },
  "homepage": "https://github.com/brunobesson/tracker-connect#readme",
  "dependencies": {
    "koa": "2.13.1",
    "koa-helmet": "6.1.0",
    "koa-pino-logger": "3.0.0",
    "koa-router": "10.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.0",
    "@types/koa": "2.13.1",
    "@types/koa-pino-logger": "3.0.0",
    "@types/koa-router": "7.4.2",
    "@types/node": "14.14.43",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "nodemon": "2.0.7",
    "pino-pretty": "4.7.1",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
